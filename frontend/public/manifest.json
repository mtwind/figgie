{
  "manifest_version": 3,
  "name": "Figgie Genius",
  "version": "1.0",
  "description": "An extension to provide analysis for the Figgie game.",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "sidePanel",
    "tabs"
  ],
  "action": {
    "default_title": "Click to open Figgie Genius"
  },
  "icons": {
    "16": "figgie.png",
    "48": "figgie.png",
    "128": "figgie.png"
  }
}